{
  "name": "rakutentools",
  "displayName": "UO 自社 Rakutentools - AI ツール",
  "version": "0.0.1",
  "description": "Rakuten RMS AI回复助手",
  "author": "UO",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "build:firefox": "plasmo build --target=firefox-mv2",
    "package": "plasmo package"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/x": "^2.2.1",
    "@ant-design/x-sdk": "^2.2.1",
    "@mermaid-js/parser": "^0.6.3",
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.12.0",
    "antd": "^6.2.2",
    "dayjs": "^1.11.19",
    "mermaid": "10.9.3",
    "optional": "^0.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-rnd": "^10.5.2"
  },
  "devDependencies": {
    "@types/chrome": "0.1.32",
    "@types/node": "25.0.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "^10.4.22",
    "plasmo": "0.90.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "5.9.3"
  },
  "manifest": {
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "https://*.rakuten.co.jp/*",
      "https://api.openai.com/*",
      "https://generativelanguage.googleapis.com/*",
      "https://open.manus.im/*",
      "https://api.manus.im/*",
      "https://api.zenmux.ai/*",
      "http://*/*",
      "https://*/*"
    ]
  },
  "pnpm": {
    "overrides": {
      "mermaid": "10.9.3"
    }
  },
  "packageManager": "pnpm@10.12.1"
}
